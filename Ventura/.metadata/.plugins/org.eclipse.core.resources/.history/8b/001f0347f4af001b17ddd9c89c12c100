public class PaginaPrincipal extends JPanel{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public void paginaPrincipal() {
		grid = new GridBagLayout();
		this.crearVentana();
	}
	
	public void crearVentana() {
		this.add(new jButton("Hola"));
	}
}
